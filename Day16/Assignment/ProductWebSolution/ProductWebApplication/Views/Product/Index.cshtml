@model IEnumerable<ProductWebApplication.Models.Product>

@{
    ViewData["Title"] = "Product List";
}


<h1>
    Product List
</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Product ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Quantity</th>
            <th>Remarks</th>
            <th>Supplier ID</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ProdId</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
                <td>@item.Qty</td>
                <td>@item.Remarks</td>
                <td>@item.SuppId</td>
            </tr>
        }
    </tbody>
</table>

<a href="../Product/Insert" class="btn btn-warning">Insert New</a>

